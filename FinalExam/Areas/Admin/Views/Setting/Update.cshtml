@using FinalExam.Areas.Admin.ViewModels.Settings
@model UpdateSettingsVM
<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Create Blog</h4>
            <form class="forms-sample" method="post" enctype="multipart/form-data">
               
                <div class="form-group">
                    <label asp-for="Key">Key</label>
                    <input asp-for="Key" type="text" class="form-control" placeholder="Key">
                    <span asp-validation-for=Key></span>
                </div>

                <div class="form-group">
                    <label asp-for="Value">Value</label>
                    <input asp-for="Value" type="text" class="form-control" placeholder="Value">
                    <span asp-validation-for=Value></span>
                </div>

                <button type="submit" class="btn btn-primary me-2">Submit</button>
                <button asp-action="Index" asp-controller="Setting" class="btn btn-light">Cancel</button>
            </form>
        </div>
    </div>
</div>